<nav aria-label="breadcrumb">
    <ol class="breadcrumb">
        <li class="breadcrumb-item" aria-current="page">Informe Ubicación Tiendas </li>
    </ol>
</nav>

<div class="row">
    <div class="col-sm-12">
        <div class="card border-dark">
            <div class="card-header text-white bg-dark">
                <h5 class="float-left">Resultados</h5>
            </div>
            <div class="card-body card-resultados">
                <div id="main" style="width: 100%;height:400px;"></div>
                <script>
                    function initMap() {
                        var map;
                        map = new google.maps.Map(document.getElementById('main'), {
                            center: { lat: 25.06, lng: -77.345 },
                            zoom: 8
                        });
                    }
                </script>
                <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyD-kq_BFil644A2VK5u5Iz0O0Priyc1Xk8&callback=initMap"
                        async defer></script>
            </div>
        </div>
        <div class="card-body">
            <table id="tbdata" class="table table-striped table-bordered" style="width:100%">
                <thead>
                    <tr>
                        <th>Nombre</th>
                        <th>CoordenadasX</th>
                        <th>CoordenadasY</th>
                        <th></th>
                    </tr>
                </thead>
                <tbody>
                </tbody>
            </table>
        </div>
    </div>
</div>

@section scripts{
    <script src="@Url.Content("~/Scripts/Views/InformeMaps_Index.js")" type="text/javascript"></script>

}